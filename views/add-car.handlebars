<div class="content add-car">

	<div class="info">
		<div class="place">
			<h2>Dodawanie pojazdu</h2>
		</div>

		<div class="breadcrumbs">
			<h3>jesteś tutaj: <a href="">Mój garaż</a><span>></span><a href="">Dodaj pojazd</a></h3>

		</div>
	</div>

	<div class="container">
		<form class="form-horizontal form" role="form">
			<h3>Dodaj pojazd</h3>
			<div class="form-group">
			    <label class="control-label col-sm-4 col-sm-offset-1" for="country">Kraj rejestracji:</label>
			    <div class="col-sm-3">
		      		<input type="text" class="form-control" id="country" autofocus>
		   		</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="year">Rok produkcji:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="year">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="brand">Marka:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="brand">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="model">Model:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="model">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="reg">Numer rejestracyjny:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="reg">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="check">Przegląd techniczny:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="check">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="insurance">Ubezpiecznie:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="insurance">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="act-km">Aktualny przebieg:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="act-km">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="avg-km">Średni roczny przebieg:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="avg-km">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1" for="vin">Numer VIN:</label>
		    	<div class="col-sm-3">
		      		<input type="text" class="form-control" id="vin">
		    	</div>
		  	</div>
		  	<div class="form-group">
		    	<label class="control-label col-sm-4 col-sm-offset-1">Jaki rodzaj opon używasz?</label>
		    	<div class="col-sm-3">
		      		<label class="radio-inline"><input type="radio" name="tire-kind" checked>Letnie</label>
					<label class="radio-inline"><input type="radio" name="tire-kind">Zimowe</label>
					<label class="radio-inline"><input type="radio" name="tire-kind">Uniwersalne</label>
		    	</div>
		  	</div>
		  	<div class="form-group small">
		    	<label class="control-label col-sm-4 col-sm-offset-1 fat big">Rozmiar opon:</label>
		    	<div class="col-sm-4">
		    		<div class="row">
			      		<label class="control-label col-sm-2 fat">Przód:</label>
			      		<label class="control-label col-sm-2" for="tire-width-front">Szerokość:</label>
			      		<input type="text" class="form-control col-sm-2" id="tire-width-front">
			      		<label class="control-label col-sm-2" for="tire-inch-front">Przekątna:</label>
			      		<input type="text" class="form-control col-sm-2" id="tire-inch-front">
			      	</div>
			      	<div class="row">
			      		<label class="control-label col-sm-2 fat">Tył:</label>
			      		<label class="control-label col-sm-2" for="tire-width-back">Szerokość:</label>
			      		<input type="text" class="form-control col-sm-2" id="tire-width-back">
			      		<label class="control-label col-sm-2" for="tire-inch-back">Przekątna:</label>
			      		<input type="text" class="form-control col-sm-2" id="tire-inch-back">
			      	</div>
		    	</div>
		  	</div>


		  	<div class="form-group">
	      		<button type="submit" class="btn btn-default col-sm-2 col-sm-offset-5">Dodaj</button>
		  	</div>
		</form>
	</div>

</div>

